<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_aboutme"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/about_backold" >

<Button
    android:id="@+id/about_back"
    android:visibility="invisible"
    android:layout_marginLeft="11dp"
    android:layout_width="50dp"
    android:layout_height="50dp"
    android:layout_marginTop="8dp"
    android:background="@drawable/aboutme_back1"
    />

    <ImageView
        android:id="@+id/my_phonenum"
        android:layout_width="300dp"
        android:layout_height="412dp"
        android:layout_gravity="center"
        android:visibility="gone"
        android:background="@drawable/connect_dialog"
        />

    <Button
        android:id="@+id/about_connectme"
        android:visibility="invisible"
        android:layout_marginLeft="30dp"
        android:layout_width="160dp"
        android:layout_height="104dp"
        android:layout_marginTop="370dp"
        android:background="@drawable/connectme_1"
        />

</LinearLayout>
